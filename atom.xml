---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
	<title type="text" xml:lang="en">Bill Thornton</title>
	<link type="application/atom+xml" href="http://billt.me/atom.xml" rel="self"/>
	<link href="http://billt.me"/>
	<updated>{{ site.time | date_to_xmlschema }}</updated>
	<id>http://billt.me</id>
	<author>
		<name>Bill Thornton</name>
	</author>

	{% for post in site.posts %}
	<entry>
		<title>{{ post.title }}</title>
		<link href="http://billt.me{{ post.url }}"/>
		<updated>{{ post.date | date_to_xmlschema }}</updated>
		<id>http://billt.me{{ post.id }}</id>
		<content type="html">{{ post.content | xml_escape }}</content>
	</entry>
	{% endfor %}
</feed>
